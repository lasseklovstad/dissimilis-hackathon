{
  "extends": [
    "react-app",
    "react-app/jest",
    "prettier",
    "prettier/react",
    "prettier/@typescript-eslint",
    "prettier/prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "func-style": [
      "error",
      "expression"
    ],
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      }
    ]
  }
}