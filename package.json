{
    "name": "DCN",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4.11.4",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.58",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/user-event": "^13.1.9",
        "@tonaljs/tonal": "^4.6.0",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.7",
        "@types/react-router-dom": "^5.1.7",
        "axios": "^0.21.1",
        "dotenv": "^10.0.0",
        "env-cmd": "^10.1.0",
        "i18next": "^20.3.1",
        "i18next-browser-languagedetector": "^6.1.2",
        "react": "^17.0.2",
        "react-app-polyfill": "^2.0.0",
        "react-dom": "^17.0.2",
        "react-i18next": "^11.10.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.3",
        "typescript": "^4.3.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test-final": "react-scripts test --watchAll=false",
        "eject": "react-scripts eject",
        "format": "prettier --write src/**/*.ts{,x}",
        "lint": "tsc --noEmit && eslint --fix src/**/*.ts{,x}",
        "build:testing": "env-cmd -f .env.testing react-scripts build",
        "build:staging": "env-cmd -f .env.staging react-scripts build",
        "build:test": "env-cmd -f ./environments/.env.test react-scripts build",
        "build:development": "env-cmd -f ./environments/.env.development react-scripts build",
        "build:production": "env-cmd -f ./environments/.env.production react-scripts build"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/react": "^11.2.7",
        "@types/jest": "^26.0.23",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest-environment-jsdom-sixteen": "^2.0.0",
        "msw": "^0.28.2",
        "prettier": "2.3.2",
        "react-app-env": "^1.2.3"
    }
}
