{
    "name": "DCN",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {
        "@emotion/react": "11.7.1",
        "@emotion/styled": "11.6.0",
        "@mui/icons-material": "5.4.2",
        "@mui/material": "5.4.2",
        "@mui/styles": "5.4.2",
        "axios": "0.26.0",
        "env-cmd": "10.1.0",
        "i18next": "21.6.11",
        "react": "17.0.2",
        "react-app-polyfill": "3.0.0",
        "react-dom": "17.0.2",
        "react-hotkeys-hook": "3.4.4",
        "react-i18next": "11.15.4",
        "react-router": "6.2.1",
        "react-router-dom": "6.2.1",
        "react-scripts": "5.0.0",
        "typescript": "4.5.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test-final": "react-scripts test --watchAll=false",
        "eject": "react-scripts eject",
        "format": "prettier --write src/**/*.ts{,x}",
        "lint": "tsc --noEmit && eslint --fix src/**/*.ts{,x}",
        "build:testing": "env-cmd -f .env.testing react-scripts build",
        "build:staging": "env-cmd -f .env.staging react-scripts build",
        "build:test": "env-cmd -f ./environments/.env.test react-scripts build",
        "build:development": "env-cmd -f ./environments/.env.development react-scripts build",
        "build:production": "env-cmd -f ./environments/.env.production react-scripts build"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.2",
        "@testing-library/react": "12.1.3",
        "@testing-library/user-event": "13.5.0",
        "@types/jest": "27.4.0",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "@types/react-router-dom": "5.3.3",
        "eslint-config-prettier": "8.4.0",
        "eslint-plugin-prettier": "4.0.0",
        "msw": "0.38.1",
        "prettier": "2.5.1"
    }
}
