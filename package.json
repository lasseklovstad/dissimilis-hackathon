{
    "name": "DCN",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {
        "@emotion/react": "11.9.0",
        "@emotion/styled": "11.8.1",
        "@mui/icons-material": "5.6.2",
        "@mui/material": "5.7.0",
        "@mui/styles": "5.7.0",
        "axios": "0.27.2",
        "env-cmd": "10.1.0",
        "i18next": "21.8.0",
        "react": "17.0.2",
        "react-app-polyfill": "3.0.0",
        "react-dom": "17.0.2",
        "react-hotkeys-hook": "3.4.4",
        "react-i18next": "11.16.9",
        "react-router-dom": "6.3.0",
        "react-scripts": "5.0.1",
        "typescript": "4.6.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test-final": "react-scripts test --watchAll=false --coverage --reporters=jest-junit",
        "eject": "react-scripts eject",
        "format": "prettier --write src/**/*.ts{,x}",
        "lint": "eslint --fix src/**/*.ts{,x}",
        "build:testing": "env-cmd -f .env.testing react-scripts build",
        "build:staging": "env-cmd -f .env.staging react-scripts build",
        "build:test": "env-cmd -f ./environments/.env.test react-scripts build",
        "build:development": "env-cmd -f ./environments/.env.development react-scripts build",
        "build:production": "env-cmd -f ./environments/.env.production react-scripts build"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "12.1.3",
        "@testing-library/user-event": "13.5.0",
        "@types/jest": "27.5.0",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.0.0",
        "jest-junit": "13.2.0",
        "msw": "0.39.2",
        "prettier": "2.6.2"
    },
    "jest": {
        "coverageReporters": [
            "cobertura"
        ]
    }
}
